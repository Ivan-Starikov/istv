---
import { getCollection } from "astro:content";
import Layout from "../layouts/Layout.astro";

const versLibres = await getCollection("versLibres");
---

<Layout title="Text">
  <main>
    <h2>Верлибры</h2>
    <!-- FIX: typescript issue -->
    {versLibres.map((item) => <li>{item.data.title}</li>)}
  </main>
</Layout>
